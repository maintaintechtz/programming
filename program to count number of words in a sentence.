#include <iostream>
#include<string.h>
using namespace std;

int main()
{
    int i,count=0;
    string str;
    cout<<"ENTER THE WORDS:"<<endl;
  getline(cin,str);
  
  for(i=0;i<=str.length();i++)
  {
      if(str[i]==' ')
      count++;
  }
    
cout<<"TOTAL NUMBER OF WORDS ARE:"<<count<<endl;

    return 0;
}
